<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>use.user.LoginServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>CreateAccServlet</servlet-name>
		<servlet-class>use.user.CreateAccServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>CreateAccServlet</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>timeTableServlet</servlet-name>
		<servlet-class>use.user.timeTableServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>timeTableServlet</servlet-name>
		<url-pattern>/uploadtt</url-pattern>
	</servlet-mapping>
	
		
	<servlet>
		<servlet-name>ViewtimeTableServlet</servlet-name>
		<servlet-class>use.user.ViewTimetableServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewtimeTableServlet</servlet-name>
		<url-pattern>/Viewtt</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>QuizLecDetailsInsert</servlet-name>
		<servlet-class>quiz.lec.QuizLecDetailsInsertServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>QuizLecDetailsInsert</servlet-name>
		<url-pattern>/QuizLecDetailsInsert</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>createQuiz</servlet-name>
		<servlet-class>createQuiz.createQuizServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>createQuiz</servlet-name>
		<url-pattern>/createQuiz</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>addQuestion</servlet-name>
		<servlet-class>addQuestion.addQuestionServlet</servlet-class>
		
	</servlet>
	<servlet-mapping>
		<servlet-name>addQuestion</servlet-name>
		<url-pattern>/addQuestion</url-pattern>
	</servlet-mapping>
	
 </web-app>